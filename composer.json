{
  "name": "vanlueckn/kimai-sevdesk-plugin",
  "description": "A plugin for kimai for creating invoices in sevdesk",
  "homepage": "https://www.kimai.org/store/demo-bundle.html",
  "type": "kimai-plugin",
  "version": "0.2",
  "keywords": [
    "kimai",
    "kimai-plugin",
    "sevdesk",
    "invoice"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nils van LÃ¼ck",
      "email": "nils@vanlueck.dev",
      "homepage": "https://vanlueck.dev"
    }
  ],
  "autoload": {
    "psr-4": {
      "KimaiPlugin\\KimaiSevdeskBundle\\": ""
    }
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "extra": {
    "kimai": {
      "require": "1.17",
      "version": "0.2",
      "name": "KimaiSevdeskBundle"
    }
  },
  "require-dev": {
    "kevinpapst/kimai2": "1.17",
    "symfony/console": "^4.4",
    "symfony/event-dispatcher": "^4.4",
    "symfony/http-client": "^4.4"
  }
}
